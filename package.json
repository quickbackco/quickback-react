{
  "name": "@quickbackco/quickback-react",
  "version": "0.2.10",
  "private": false,
  "main": "dist/quickback.js",
  "dependencies": {
    "react": "^18.2.0",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src/quickback.js --out-dir dist --copy-files && babel src/quickback.css --out-dir dist/css --copy-files",
    "start": "react-scripts start",
    "publish": "npm publish --access=public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "keywords": [
    "quickback",
    "feedback",
    "feedbacks",
    "customers",
    "clients",
    "customer",
    "client"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quickbackco/quickback-react.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/preset-react": "^7.18.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
